squid:
  example_rule: True

  http_port:
     - 3128
     - 127.0.0.1:8080

  cache_mgr_user: username
  cache_mgr_password: password

  src_acl:
    src_aclname:
      - src1
      - src2

  port_acl:
    port_aclname:
      - port1
      - port2

  dst_acl:
    dst_aclname:
      - dst1
      - dst2

  dstdomain_acl:
    dstdomain_aclname:
      - .example.com
      - .example.org

  http_access:
    - allow src1
    - deny src2 dstdomain_aclname
    - allow all

  cache_dir: cache_dir ufs /home/squid/cache 1000 16 256

  visible_hostname: some.name

  extra_configuration: |
    refresh_pattern bla       0   20% 4320
    refresh_pattern foo       0   20% 4320
